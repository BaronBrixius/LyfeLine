      <Label text="Title"/>
      <TextArea fx:id="titleInput" prefHeight="51.0" prefWidth="195.0"/>
      <Label layoutX="10.0" layoutY="10.0" text="Description"/>
      <TextArea fx:id="descriptionInput" layoutX="10.0" layoutY="27.0" prefHeight="51.0" prefWidth="195.0"/>
      <Label layoutX="10.0" layoutY="78.0" text="KeyWords"/>
      <TextArea fx:id="keywordsInput" layoutX="10.0" layoutY="95.0" prefHeight="51.0" prefWidth="195.0"/>
      <Label layoutX="10.0" layoutY="146.0" text="Start Date"/>
      <DatePicker fx:id="startDateInput"/>
      <Label layoutX="10.0" layoutY="214.0" text="End Date"/>
      <DatePicker fx:id="endDateInput"/>
      <Label layoutX="10.0" layoutY="282.0" text="Time Units"/>
      <ComboBox fx:id="timeInput" prefWidth="150.0">
      <items>
         <FXCollections fx:factory="observableArrayList">
            <String fx:value="Seconds"/> <String fx:value="Minutes"/> <String fx:value="Hours"/> <String fx:value="Days"/> <String fx:value="Years"/>
         </FXCollections>
      </items>
   </ComboBox>
</VBox>
</GridPane>
