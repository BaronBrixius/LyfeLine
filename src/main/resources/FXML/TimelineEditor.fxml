<GridPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="controllers.TimelineEditor">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="733.3333282470703" minWidth="10.0" prefWidth="365.0"/>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="653.3333740234375" minWidth="0.0" prefWidth="375.0"/>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="660.0" minWidth="10.0" prefWidth="495.6666259765625"/>
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="729.0" minHeight="10.0" prefHeight="634.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="330.0" minHeight="10.0" prefHeight="112.0" vgrow="SOMETIMES"/>
   </rowConstraints>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <HBox alignment="CENTER" prefHeight="166.0" prefWidth="533.0" spacing="10.0" GridPane.columnIndex="2"
         GridPane.rowIndex="1">
      <Button mnemonicParsing="false" onMouseClicked="#save" text="Save"/>
      <Button mnemonicParsing="false" onMouseClicked="#cancel" text="Cancel"/>
   </HBox>
   <VBox prefHeight="737.0" prefWidth="0.0" spacing="10.0"
         style="-fx-background-color: gray; -fx-border-color: black; -fx-border-width: 3;" GridPane.rowSpan="2">
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
