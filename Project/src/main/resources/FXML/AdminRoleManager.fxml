           </HBox>
           <ComboBox fx:id="sortBy" nodeOrientation="LEFT_TO_RIGHT" prefHeight="25.0" prefWidth="328.0" promptText="Sort by" GridPane.columnIndex="1" GridPane.rowIndex="1" />
