            <CheckBox fx:id="toggle" mnemonicParsing="false" onAction="#toggleClicked" styleClass="mediumText" text="Admin" GridPane.columnIndex="1" />
      </GridPane>
   </center>
   <bottom>
      <GridPane BorderPane.alignment="CENTER">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="304.0" minWidth="10.0" prefWidth="250.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="386.0" minWidth="10.0" prefWidth="250.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
              <Button alignment="CENTER" mnemonicParsing="false" onAction="#back" styleClass="dashBoard" text="Back" GridPane.halignment="CENTER" />
