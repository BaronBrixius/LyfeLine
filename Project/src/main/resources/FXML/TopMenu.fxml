<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar maxHeight="25.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TopMenu">
   <Menu mnemonicParsing="false" text="File">
      <MenuItem fx:id="saveButton" mnemonicParsing="false" onAction="#saveFile" text="Save" />
      <MenuItem mnemonicParsing="false" text="Delete" />
      <MenuItem mnemonicParsing="false" text="Zoom" />
   </Menu>
   <Menu mnemonicParsing="false" text="Edit">
      <MenuItem mnemonicParsing="false" text="Edit Mode" />
   </Menu>
   <Menu mnemonicParsing="false" text="View">
      <MenuItem mnemonicParsing="false" text="View Mode" />
   </Menu>
</MenuBar>
