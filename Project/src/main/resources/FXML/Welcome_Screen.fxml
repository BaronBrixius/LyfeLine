<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="800.0" prefWidth="1280.0"
      spacing="100.0"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="LoginAndRegistration_GUI">
    <MenuBar VBox.vgrow="NEVER">
        <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" text="Save"/>
            <MenuItem mnemonicParsing="false" text="Delete"/>
            <MenuItem mnemonicParsing="false" text="Zoom"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem mnemonicParsing="false" text="Edit Mode"/>
        </Menu>
        <Menu mnemonicParsing="false" text="View">
            <MenuItem mnemonicParsing="false" text="View Mode"/>
        </Menu>
    </MenuBar>
    <HBox alignment="CENTER" fillHeight="false" prefHeight="100.0" prefWidth="1280.0" spacing="10.0">
        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#loginScreen"
                style="-fx-font-size: 2em;" text="Login" textAlignment="CENTER"/>
        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-font-size: 2em;"
                text="Register" textAlignment="CENTER" onMouseClicked="#registerScreen"/>
        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-font-size: 2em;"
                text="Continue as guest" textAlignment="CENTER"/>
        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Timelines"
                textAlignment="CENTER" onMouseClicked="#timelineScreen"/>
    </HBox>
    <ImageView fitHeight="411.0" fitWidth="880.0" preserveRatio="true">
        <Image url="@../Logo.png"/>
    </ImageView>
</VBox>
